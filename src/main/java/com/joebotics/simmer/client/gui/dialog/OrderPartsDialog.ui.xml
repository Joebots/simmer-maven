<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:m="urn:import:gwt.material.design.client.ui">
    <ui:with field='i18n' type='com.joebotics.simmer.client.util.I18N' />

    <m:MaterialModal ui:field="modal" type="DEFAULT" dismissible="true" inDuration="500" outDuration="500"
                     styleName="shifted-dialog">
        <m:MaterialButton ui:field="close" style="float: right;" styleName="right-align"
                          iconPosition="RIGHT" backgroundColor="WHITE" textColor="BLACK" text="X"></m:MaterialButton>
        <g:Frame url="http://localhost:4200/send-parts" styleName="bordered" width="100%" height="440px"></g:Frame>
    </m:MaterialModal>
</ui:UiBinder>
