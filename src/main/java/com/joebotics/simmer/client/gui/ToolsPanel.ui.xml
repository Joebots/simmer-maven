<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:jbd="urn:import:com.joebotics.simmer.client.gui.dialog">
    <ui:with field='i18n' type='com.joebotics.simmer.client.util.I18N'/>
    <ui:style>
    </ui:style>
    <g:HTMLPanel>
        <g:HTMLPanel styleName="row">
            <g:HTMLPanel styleName="center white-text">
                <g:HTMLPanel styleName="col m5ths">
                    <m:MaterialButton ui:field="schematicButton" type="FLOATING" waves="LIGHT" size="LARGE"
                                      iconType="ADD"/>
                    <g:HTMLPanel>
                        Add Element
                    </g:HTMLPanel>
                </g:HTMLPanel>
                <g:HTMLPanel styleName="col m5ths">
                    <m:MaterialButton ui:field="fileButton" type="FLOATING" waves="LIGHT" size="LARGE"
                                      iconType="FOLDER"/>
                    <g:HTMLPanel>
                        Open
                    </g:HTMLPanel>
                </g:HTMLPanel>
                <g:HTMLPanel styleName="col m5ths">
                    <m:MaterialButton ui:field="optionsButton" type="FLOATING" waves="LIGHT" size="LARGE"
                                      iconType="SETTINGS"/>
                    <g:HTMLPanel>
                        Settings
                    </g:HTMLPanel>
                </g:HTMLPanel>
                <g:HTMLPanel styleName="col m5ths">
                    <m:MaterialButton ui:field="programmingButton" type="FLOATING" waves="LIGHT" size="LARGE"
                                      iconType="EXTENSION"/>
                    <g:HTMLPanel>
                        Code Blocks
                    </g:HTMLPanel>
                </g:HTMLPanel>
                <g:HTMLPanel styleName="col m5ths">
                    <m:MaterialButton ui:field="powerButton" type="FLOATING" waves="LIGHT" size="LARGE"
                                      iconType="POWER_SETTINGS_NEW"/>
                    <g:HTMLPanel>
                        Exit
                    </g:HTMLPanel>
                </g:HTMLPanel>
            </g:HTMLPanel>
        </g:HTMLPanel>
        <jbd:SchematicDialog ui:field="schematicDialog"/>
        <jbd:CircuitsDialog ui:field="circuitsDialog"/>
        <jbd:OptionsDialog ui:field="optionsDialog"/>
        <jbd:ProgrammingDialog ui:field="programmingDialog"/>
        <jbd:ExitDialog ui:field="exitDialog"/>
    </g:HTMLPanel>
</ui:UiBinder>
