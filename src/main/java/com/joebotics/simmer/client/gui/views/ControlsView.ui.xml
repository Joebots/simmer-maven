<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:m='urn:import:gwt.material.design.client.ui'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:g.table='urn:import:com.google.gwt.user.cellview.client'
             xmlns:jbd="urn:import:com.joebotics.simmer.client.gui.dialog">

    <m:MaterialPanel height="100%">
        <m:MaterialRow>
            <m:MaterialPanel backgroundColor="WHITE">
                <g:AbsolutePanel ui:field="controlsContainer" styleName="controls-grid">
                </g:AbsolutePanel>
            </m:MaterialPanel>
        </m:MaterialRow>
        <m:MaterialRow>
            <m:MaterialColumn grid="s12" backgroundColor="WHITE">
                <m:MaterialPanel ui:field="controlsListContainer">
                    <g.table:CellTable ui:field="table"/>
                </m:MaterialPanel>
            </m:MaterialColumn>
        </m:MaterialRow>
        <jbd:EditControlDialog ui:field="editControlDialog"/>
    </m:MaterialPanel>
</ui:UiBinder>